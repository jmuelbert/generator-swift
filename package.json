{
  "name": "generator-swift",
  "version": "0.1.0",
  "description": "Yeoman generator for opensource swift projects",
  "author": "J端rgen M端lbert",
  "license": "Apache-2.0",
  "main": "app/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmuelbert/generator-swift.git"
  },
  "scripts": {
    "test": "jest",
    "pretest": "eslint . --fix",
    "prepublish": "nsp check"
  },
  "contributors": [
    "J端rgen M端lbert < > (https://github.com/jmuelbert)"
  ],
  "engines": {
    "node": ">=4.0.0"
  },
  "keywords": [
    "yeoman-generator",
    "swift"
  ],
  "files": [
    "app"
  ],
  "dependencies": {
    "chalk": "^2.1.0",
    "deep-extend": "^0.6.0",
    "generator-node": "^2.3.0",
    "inquirer-npm-name": "^3.0.0",
    "lint-staged": "^7.2.2",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "superb": "^3.0.0",
    "yeoman-generator": "^3.1.1",
    "yosay": "^2.0.1"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint": "^5.3.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-xo-space": "^0.20.0",
    "eslint-plugin-prettier": "^2.6.0",
    "handlebars": "^3.0.0",
    "jest": "^23.5.0",
    "jest-cli": "^23.5.0",
    "nsp": "^3.2.1",
    "prettier": "^1.10.2",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "eslintConfig": {
    "extends": [
      "xo-space/esnext",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "templates"
    ]
  },
  "bugs": {
    "url": "https://github.com/jmuelbert/generator-swift/issues"
  },
  "homepage": "https://github.com/jmuelbert/generator-swift#readme",
  "directories": {
    "test": "test"
  }
}
