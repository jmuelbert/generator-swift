{
  "name": "generator-swift",
  "version": "0.1.0",
  "description": "Yeoman generator for opensource swift projects",
  "author": "J端rgen M端lbert",
  "license": "Apache-2.0",
  "main": "app/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmuelbert/generator-swift.git"
  },
  "scripts": {
    "test": "jest",
    "format": "prettier generators/**/*.js test/**/*.js --write",
    "lint": "eslint generators/**/*.js test/**/*.js --fix",
    "precommit": "lint-staged && npm test",
    "prepublishOnlyOnly": "lint-staged && npm test",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "contributors": [
    "J端rgen M端lbert < > (https://github.com/jmuelbert)"
  ],
  "engines": {
    "node": ">=4.0.0"
  },
  "keywords": [
    "yeoman-generator",
    "swift"
  ],
  "files": [
    "app"
  ],
  "lint-staged": {
    "*.js": [
      "npm run format",
      "npm run lint",
      "git add"
    ]
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "deep-extend": "^0.6.0",
    "generator-node": "^2.6.0",
    "inquirer-npm-name": "^3.0.0",
    "lint-staged": "^9.5.0",
    "lodash": "^4.17.15",
    "mkdirp": "^0.5.1",
    "superb": "^4.0.0",
    "yeoman-generator": "^4.0.1",
    "yo": "^3.1.1",
    "yosay": "^2.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "babel-jest": "^24.9.0",
    "coveralls": "^3.0.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-xo-space": "^0.22.0",
    "eslint-plugin-mocha": "6.2.2",
    "eslint-plugin-prettier": "^3.1.0",
    "fs-extra": "^8.1.0",
    "handlebars": "^4.5.3",
    "jest": "^26.4.0",
    "nsp": "^3.2.1",
    "prettier": "^1.18.2",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^2.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "templates"
    ]
  },
  "bugs": {
    "url": "https://github.com/jmuelbert/generator-swift/issues"
  },
  "homepage": "https://github.com/jmuelbert/generator-swift#readme",
  "directories": {
    "test": "test"
  }
}
